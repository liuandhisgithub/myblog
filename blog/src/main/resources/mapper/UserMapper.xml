<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liu.myblog.dao.UserDao">
    <insert id="insertAUser" parameterType="com.liu.myblog.entity.User">
        insert into user(id,password,create_time) value (#{id},#{password},#{createTime})
    </insert>
</mapper>